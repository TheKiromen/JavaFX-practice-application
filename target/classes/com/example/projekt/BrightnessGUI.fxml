<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" maxHeight="180.0" maxWidth="480.0" minHeight="180.0" minWidth="480.0" prefHeight="180.0" prefWidth="480.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projekt.BrightnessAlertController">
   <children>
      <Label text="Brightness">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Slider fx:id="brightness" blockIncrement="0.1" majorTickUnit="0.2" max="1.0" min="-1.0" minorTickCount="10" showTickLabels="true" snapToTicks="true">
         <VBox.margin>
            <Insets bottom="30.0" left="20.0" right="20.0" top="5.0" />
         </VBox.margin>
      </Slider>
      <Button fx:id="confirm" mnemonicParsing="false" onAction="#confirm" text="Confirm" />
   </children>
</VBox>
